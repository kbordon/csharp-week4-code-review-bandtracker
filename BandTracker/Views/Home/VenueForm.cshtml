@{
    Layout = "_Layout";
}
<h1>ADD A VENUE</h1>
<hr />
<form action="/venues/new" method="post">
  <label for="venue-name">NAME:</label>
  <input id="venue-name" name="venue-name" type="text" pattern="^[A-Z0-9][A-Za-z0-9_@@. /#&+-$@@]*$">
  <button type="submit">ADD</button>
</form>
<p><a href="/venues">VIEW ALL VENUES</a></p>
<p><a href="/">HOME</a></p>
